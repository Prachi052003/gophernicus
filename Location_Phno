import phonenumbers
from phonenumbers import geocoder
phonenumbers1 = phonenumbers.parse("+919381459415")
phonenumbers2 = phonenumbers.parse("+918147296345")

print("\nPhone number location\n")

print(geocoder.description_for_number(phonenumbers1,"en"))
print(geocoder.description_for_number(phonenumbers2,"en"))
